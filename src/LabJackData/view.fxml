<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LabJackData.Controller">
   <children>
      <Pane prefHeight="459.0" prefWidth="806.0">
         <children>
            <Button fx:id="armButton" layoutX="152.0" layoutY="366.0" mnemonicParsing="false" onAction="#armButtonPress" prefHeight="59.0" prefWidth="163.0" text="Arm">
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Button fx:id="disarmButton" disable="true" layoutX="322.0" layoutY="366.0" mnemonicParsing="false" onAction="#disarmButtonPress" prefHeight="59.0" prefWidth="163.0" text="Disarm">
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Button fx:id="ignitionButton" disable="true" layoutX="492.0" layoutY="366.0" mnemonicParsing="false" onAction="#ignitionButtonPress" prefHeight="59.0" prefWidth="163.0" text="Ignition">
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Label layoutX="152.0" layoutY="430.0" prefHeight="17.0" prefWidth="110.0" text="Connection Status:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <Label fx:id="connectionStatus" layoutX="266.0" layoutY="430.0" text="Disconnected" textFill="RED" />
            <TextArea fx:id="logTextArea" editable="false" layoutX="153.0" layoutY="186.0" prefHeight="171.0" prefWidth="501.0" wrapText="true" />
            <Label layoutX="152.0" layoutY="165.0" text="Output log">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
